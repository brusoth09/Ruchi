<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ruchi.hibernate.model.DAO.FoodDao" table="foods">
        <id name="food_id" type="java.sql.Timestamp">
            <column name="food_id" />
            <!--<generator class="increment" />-->
        </id>
        <property name="food_name" type="java.lang.String">
            <column name="food_name" />
        </property>
        <property name="food_type" type="java.lang.String">
            <column name="food_type" />
        </property>
    </class>
</hibernate-mapping>